const express = require('express')
const util = require('util')

var path = require('path')

const app = express()
app.set('views', path,join(__dirname, 'views'))
app.set('view engine', 'hbs')


app.get('/_bicis', (req, res) => {

	console.log(">>>>>>>>>> GETING MAP")
	res.render('index')
})


module.exports = app
