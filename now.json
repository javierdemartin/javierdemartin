{
	"version": 2,
	"alias": "javierdemart.in",
"builds": [
    { "src": "resources/*", "use": "@now/static" },
    { "src": "*.html", "use": "@now/static" },
	{ "src": "*.css", "use": "@now/static" },
	{ "src": "_posts/resources/*", "use": "@now/static" },
	{ "src": "/js/*.js", "use": "@now/node-server" }
  ],
 "routes": [
  	{"src": "/", "dest": "/js/index.js"},
  	{"src": "/api/v1/prediction/(.*)", "dest": "/js/index.js"},
 	{"src": "/api/v1/today/(.*)", "dest": "/js/index.js"},
 	{"src": "/api/v1/prediction/(.*)/(.*)", "dest": "/js/index.js"},
 	{"src": "/api/v1/all/(.*)/(.*)", "dest": "/js/index.js"},
	{"src": "/api/v1/today/(.*)/(.*)", "dest": "/js/index.js"},
	{"src": "/blog", "dest": "/js/index.js"},
	{"src": "/blog/", "dest": "/js/index.js"},
	{"src": "/blog/([^/]+)", "dest": "/js/index.js"},
	{"src" : "/bicis/(.*)", "dest": "/js/index.js"},
	{"src" : "/bicis", "dest": "/js/index.js"},
	{"src": "/cv", "dest": "/resources/Javier_de_Martin_resume.pdf"}]
}
