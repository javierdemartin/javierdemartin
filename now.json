{

	"name": "javierdemartin",
	"version": 2,
	"alias": "javierdemart.in",
"builds": [
    { "src": "bicis.png", "use": "@now/static" },
    { "src": "cv.pdf", "use": "@now/static" },
    { "src": "*.html", "use": "@now/static" },
	{ "src": "*.css", "use": "@now/static" },
	{ "src": "js/*.js", "use": "@now/node-server" },
	{ "src": "*.js", 
		"use": "@now/node-server",
		"config": { "maxLambdaSize": "4000mb"}
	}
  ],
 "routes": [
	{"src" : "/bicis", "dest": "/index.js"},
	{"src": "/cv", "dest": "/cv.pdf"}]
}